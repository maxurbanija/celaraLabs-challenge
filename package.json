{
  "name": "home-test-automation",
  "version": "1.0.0",
  "description": "Simple test automation with Playwright and TypeScript",
  "scripts": {
    "test": "npx playwright test --reporter=html,line",
    "test:headed": "npx playwright test --headed",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "devDependencies": {
    "@playwright/test": "1.46.1",
    "@types/node": "^24.0.11",
    "prettier": "^3.6.2",
    "typescript": "^5.0.0"
  }
}
